#!/usr/bin/env node

// These ./bin files are customized for stackblitz
// A local Script Kit install works a little differently
let childProcess = require("child_process")

let [, , ..._args] = process.argv
childProcess.spawnSync(
  "node",
  ["./{{command}}.mjs", ..._args],
  {
    cwd: __dirname,
    stdio: "inherit",
  }
)
